csubdir=	clib
jsubdir=	javalib

all:
	@(cd $(csubdir) && $(MAKE) all)
	@(cd $(jsubdir) && $(MAKE) all)

test:

clean:
	@(cd $(csubdir) && $(MAKE) clean)
	@(cd $(jsubdir) && $(MAKE) clean)

distclean:
	@(cd $(csubdir) && $(MAKE) distclean)
	@(cd $(jsubdir) && $(MAKE) distclean)
	rm -f Makefile .depend
	-rmdir $(csubdir) $(jsubdir)

install:
	@(cd $(csubdir) && $(MAKE) install)
	@(cd $(jsubdir) && $(MAKE) install)

derived-files:
	@(cd $(csubdir) && $(MAKE) derived-files)
	@(cd $(jsubdir) && $(MAKE) derived-files)

depend:
	@(cd $(csubdir) && $(MAKE) depend)
	@(cd $(jsubdir) && $(MAKE) depend)
